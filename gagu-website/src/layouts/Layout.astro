---
import Page from './Page.astro'
import Header from './Header.astro'
import Footer from './Footer.astro'
import '../css/index.css'

export interface LayoutProps {
  title?: string;
}

const { title } = Astro.props as LayoutProps
---

<Page title={title}>
  <Header />
  <slot />
  <Footer />
</Page>

